{
  "heading": "Charged Up - 2023",
  "logo": "logo.png",
  "forceQualifiers": "true",
  "pages": [
    {
      "name": "Setup",
      "widgets": [
        {
          "name": "Starting Position",
          "type": "positions",
          "file": "field.png"
        }
      ]
    },
    {
      "name": "Autonomous",
      "widgets": [
        {
          "name": "Co-Op Nodes",
          "type": "label",
          "align": "center",
          "col": 2
        },
        {
          "name": "Upper Nodes",
          "type": "togglegrid",
          "prefix": "Auto",
          "width": 9,
          "height": 2,
          "colors": [
            "red",
            "green"
          ]
        },
        {
          "name": "Hybrid Nodes",
          "type": "togglegrid",
          "prefix": "Auto",
          "width": 9,
          "height": 1,
          "colors": [
            "black",
            "yellow",
            "purple"
          ]
        },
        {
          "name": "Dropped",
          "type": "spinbox",
          "prefix": "Auto"
        },
        {
          "type": "spacer",
          "height": 20
        },
        {
          "name": "Left Community",
          "type": "checkbox"
        },
        {
          "name": "Crossed Charging Station",
          "type": "checkbox"
        },
        {
          "name": "Crossed Cable",
          "type": "checkbox"
        },
        {
          "name": "Charging Station",
          "prefix": "Auto",
          "type": "radio",
          "options": [
            "Not Attempted",
            "Engaged",
            "Docked",
            "Failed"
          ]
        }
      ]
    },
    {
      "name": "Teleop",
      "widgets": [
        {
          "name": "Co-Op Nodes",
          "type": "label",
          "align": "center",
          "col": 2,
          "colspan": 3
        },
        {
          "name": "Upper Nodes",
          "type": "togglegrid",
          "colspan": 3,
          "width": 9,
          "height": 2,
          "colors": [
            "red",
            "green"
          ]
        },
        {
          "name": "Hybrid Nodes",
          "type": "togglegrid",
          "colspan": 3,
          "width": 9,
          "height": 1,
          "colors": [
            "black",
            "yellow",
            "purple"
          ]
        },
        {
          "name": "Dropped",
          "type": "spinbox"
        },
        {
          "name": "Was defended",
          "type": "checkbox"
        },
        {
          "name": "Defending team",
          "type": "text"
        }
      ]
    },
    {
      "name": "Endgame",
      "widgets": [
        {
          "name": "Final Status",
          "type": "radio",
          "options": [
            "Engaged",
            "Docked",
            "Parked",
            "Out of community"
          ],
          "default": 3
        },
        {
          "name": "Robots On Charge Station",
          "type": "spinbox",
          "max": 3
        },
        {
          "name": "Links",
          "type": "spinbox"
        },
        {
          "name": "Co-op bonus",
          "type": "checkbox"
        },
        {
          "name": "Penalty points",
          "type": "number"
        },
        {
          "name": "Result",
          "type": "radio",
          "options": [
            "Won",
            "Lost",
            "Tie"
          ]
        },
        {
          "name": "Score",
          "type": "number"
        },
        {
          "name": "Comments",
          "type": "textarea"
        }
      ]
    }
  ]
}
